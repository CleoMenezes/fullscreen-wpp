{

  "manifest_version": 2,
  "name": "Maximize WhatsApp™ Web",
  "version": "2.5",

  "description": "Enable full viewport for large aspect ratio screens on WhatsApp™ Web.",

  "icons": {
    "16": "icons/maximize-whatsapp-web-icon-16.png",
    "48": "icons/maximize-whatsapp-web-icon-48.png",
    "128": "icons/maximize-whatsapp-web-icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["main.js"]
    }
  ]

}
